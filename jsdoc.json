{
    "source": {
        "include": [
            "dist"
        ],
        "excludePattern": "node_modules",
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/jsdoc",
        "readme": "./README.md",
        "template": "./node_modules/docdash"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}